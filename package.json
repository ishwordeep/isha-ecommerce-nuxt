{
  "name": "nuxt-front",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview --port 6002",
    "deploy": "npm install && npm run build && pm2 reload frontend || pm2 start npm --name frontend -- run start",
    "deploy:dev": "npm install && npm run build && pm2 reload frontend || pm2 start npm --name frontend -- run dev",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/ui": "^4.0.1",
    "@pinia/nuxt": "^0.11.2",
    "@tailwindcss/vite": "^4.1.14",
    "axios": "^1.12.2",
    "nuxt": "^4.1.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.9.2"
  }
}
